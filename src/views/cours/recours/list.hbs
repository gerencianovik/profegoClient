<link rel="stylesheet" href="">
{{#each listaPagina}}
<form action="/recours/list/{{idPage}}" method="post">
    {{/each}}
    {{#each listaPagina}}
    <div id="mas">
        <a href="/recours/add/{{idPage}}">+</a>
    </div>
    {{/each}}
    <div id="contenedor">
        <div id="titulo">
            <p>Lista de Recursos</p>
        </div>
        <div id="Datos">
            <div class="row">
                {{#each lista}}
                <div id="tarjeta">
                    <div id="nombre">
                        <p>{{nameRecours}}</p>
                    </div>
                    <div id="descripcion">
                        <p>{{descriptionRecourse}}</p>
                    </div>
                    <div id="cantidad">
                        <p>{{quantyRecours}}</p>
                    </div>
                    <div id="valor">
                        <p>{{valueRecours}}</p>
                    </div>
                    <div id="estado">
                        <p>{{stateRecours}}</p>
                    </div>
                    <div id="actualizar">
                        <a href="/recours/update/{{idRecours}}"><button type="button">Actualizar</button></a>
                    </div>
                    <div id="desavilitar">
                        <a href="/recours/delete/{{idRecours}}"><button type="button">Desabilitar</button></a>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
    </div>
</form>