<link rel="stylesheet" href="">
{{#each listaCurso}}
<form action="/cours/list/{{idCours}}" method="get">
<div id="mas">
    <a href="/cours/add/{{idCours}}"><button type="button"><img src="/img/icons/mas.png"></button></a>
</div>
    {{/each}}
    <div id="contenedor">
        <div id="titulo">
            <p>Lista de Cursos</p>
        </div>
        <div id="Datos">
            <div class="row">
                {{#each lista}}
                <div id="tarjeta">
                    <div id="foto">
                        <p><img src="/img/cours/{{photoCours}}" alt=""></p>
                    </div>
                    <div id="video">
                        <p><video src="/video/cours/{{videoCours}}" controls></video></p>
                    </div>
                    <a href="/cours/detailList/{{idCours}}">
                        <div id="Nombre">
                            <p>{{nameCours}}</p>
                        </div>
                        <div id="descripcion">
                            <p>{{descriptionCours}}</p>
                        </div>
                        <div id="fehca">
                            <p>{{dateCours}}</p>
                        </div>
                        <div id="hora">
                            <p>{{hourCours}}</p>
                        </div>
                        <div id="cupos">
                            <p>{{shareCours}}</p>
                        </div>
                        <div id="costo">
                            <p>{{costCours}}</p>
                        </div>
                        <div id="estado">
                            <p>{{stateCours}}</p>
                        </div>
                        <div id="calificacion">
                            <p>{{calificationCours}}</p>
                        </div>
                    </a>
                    <div id="actualizar">
                        <a href="/cours/update/{{idCours}}"><button type="button">Actualizar</button></a>
                    </div>
                    <div id="materialesDeClase">
                        <a href="/material/add/{{idCours}}"><button type="button">Materiales</button></a>
                    </div>
                    <div id="recursosDeClase">
                        <a href="/recours/add/{{idCours}}"><button type="button">Recursos</button></a>
                    </div>
                    <div id="desavilitar">
                        <a href="/cours/delete/{{idCours}}"><button type="button">Desabilitar</button></a>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
    </div>
</form>