<link rel="stylesheet" href="/css/teacher/lista.css">
<div id="contenidoPefil">
    <div id="fondo">
        <img src="/img/fondo/Postulación.png">
    </div>
    {{#each listaTeacher}}
    <div id="titulos">
        <p>Perfil de {{completeNmeTeacher}}</p>
    </div>
    <form action="/teacher/lista/{{idTeacher}}" method="get">
        <div id="datos">
            <div id="imagenTeacher">
                <div id="cambioImagen">
                    <img src="/img/usuario/{{photoTeacher}}" id="imagenPrevisualizacion">
                </div>
            </div>
            <div id="direccion">
                <div id="map"></div>
                <input type="text" name="addressTeacher" id="ubicacion" placeholder="Dirección"
                    value="{{addressTeacher}}" required>
            </div>
            <div id="Eleccion">
                <p>Especialidad:</p>
                <div class="row">
                    {{#each ../listaChouch}}
                    <div id="tarjeta" class="col">
                        <h5>{{stateTeachCouch}}</h5>
                    </div>
                    {{/each}}
                </div>
            </div>
            <div id="fechaNacimiento">
                <p>Fecha Nacimineto:</p>
                <h5>{{ageTeacher}}</h5>
            </div>
            <div id="tituloEspecialidad">
                <p>Titulos:</p>
                <div class="row">
                    {{#each ../listaMaterias}}
                    <div id="tarjeta" class="col">
                        <h5>{{nameSpecialType}}</h5>
                    </div>
                    {{/each}}
                </div>
            </div>
            <div id="documentacion">
                <div id="DocumentoTitulo">
                    <iframe id="documentoTituloPrevisualizacion"
                        src="/archivos/teacher/{{endorsementCertificateTeacher}}"></iframe>
                </div>
                <div id="DocumentoHojaVida">
                    <iframe id="documentoHojaVidaPrevisualizacion"
                        src="/archivos/teacher/{{pageVitalTeacher}}"></iframe>
                </div>
                <div id="DocumentoAntecedentes">
                    <iframe id="documentoAntecedentesPrevisualizacion"
                        src="/archivos/teacher/{{criminalRecordTeacher}}"></iframe>
                </div>
            </div>
            <div id="materias">
                <div class="row">
                    {{#each ../listaDetalle}}
                    <div id="tarjeta" class="col">
                        <h5>{{subjetTeacher}}</h5>
                    </div>
                    {{/each}}
                </div>
            </div>
            <div id="edadRange">
                <div class="row">
                    {{#each ../listaDetalle}}
                    <div id="tarjeta" class="col">
                        <h5>{{rangeAgeTeacher}}</h5>
                    </div>
                    {{/each}}
                </div>
            </div>
            <div id="description">
                <p>{{descriptionTeacher}}</p>
            </div>
            <div id="Cedula">
                <p>{{identificationCardTeacher}}</p>
            </div>
            <div id="Celular">
                <p>{{phoneTeacher}}</p>
            </div>
            <div id="Correos">
                <p>{{emailTeacher}}</p>
            </div>
            <div id="estado">
                <p>{{stateTeacher}}</p>
            </div>
            <div id="boton">
                <a href="/teacher/update/{{idTeacher}}"><button type="button">Actualizar Sus Datos</button></a>
            </div>
        </div>
        <div id="cuadroDebajoDelForm">
            <!-- Contenido sin nada-->

        </div>
    </form>
    {{/each}}
</div>
<!-- footer-->
<div id="footer">
    <div id="tarjetaNosotros">
        <div id="nosotrosFooter">
            <h1>Nosotros</h1>
        </div>
        <div id="datosNosotros">
            <p>¿Quiénes Somos?<br>
                <br>
                <br>
                ¿Cómo funciona?<br>
                <br>
                <br>
                Testimonios<br>
                <br>
                <br>
                Preguntas frecuentes<br>
                <br>
                <br>
                Contacto
            </p>
        </div>
    </div>
    <div id="tarjetaUnete">
        <div id="nosotrosFooter">
            <h1>Únete</h1>
        </div>
        <div id="datosNosotros">
            <p>Regístrate como estudiante <br>
                <br>
                <br>
                Sé docente o coach
            </p>
        </div>
    </div>
    <div id="tarjetaLegal">
        <div id="nosotrosFooter">
            <h1>Legal</h1>
        </div>
        <div id="datosNosotros">
            <p>Términos y condiciones <br>
                <br>
                <br>
                Políticas de privacidad
            </p>
        </div>
    </div>
    <div id="logoImagen">
        <img src="/img/logo/LogoPrincipal.png">
    </div>
    <div id="final">
        <p>Interconexión segura, rápida e ideal entre docentes y estudiantes.</p>
        <p id="finalFinal">ProfeGo® EDUCONECTA S.A.S</p>
    </div>
</div>
</div>

<script
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB_Q9PaOYlXpeqtj_0tHE19Zcoim25iSFg&callback=initAutocomplete&libraries=places&v=weekly"
    defer></script>
<script src="/js/mapaDetalle.js"></script>